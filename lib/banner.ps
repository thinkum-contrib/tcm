%!
%	cs.vu.nl banner page
%
save
/notrail {
	dup length 1 sub -1 0
	{ dup 0 eq { pop pop () exit } if 2 copy get 32 ne { 1 add 0 exch getinterval exit } { pop} ifelse } for
} bind def

/LS {
	(Job: ) search {
		(:)search {
			notrail /h exch def pop
			notrail /n exch def
		}{
			/h () def
			notrail /n exch def
		} ifelse
		pop
	}{
		/h () def
		/n () def
	} ifelse
	(Date: ) search {
		notrail /j exch def
		pop
		notrail /d exch def
	}{
		/j () def
		/d () def
		pop
	} ifelse
	0.4 setlinewidth
	/ds {
		72 0 rlineto
		0 800 rlineto
		-72 0 rlineto
		closepath
		0.75 setgray
		gsave
			fill
		grestore
		0 setgray
		stroke
	} def
	20 20 moveto
	ds
	newpath
	    500 20 moveto
	ds
	/Palatino-Bold findfont 30 scalefont setfont
	gsave
		28 816 translate
		-90 rotate
		n fo
	grestore
	gsave
		564 24 translate
		90 rotate
		n fo
	grestore
	j (stdin) ne
	{
		/Helvetica findfont
		18 scalefont setfont
		296 500 moveto
		j center-show
	} if
        .1 setgray
	/Helvetica findfont 18 scalefont setfont
        296 425 moveto
	d center-show
        296 350 moveto
	/Helvetica findfont 14 scalefont setfont
	h center-show
	showpage
	statusdict begin newsheet end
	/LS { pop } def
} def

/fo {
	9 {
		dup
		gsave
			0 0 moveto
			show
		grestore
		0 36 translate
		currentgray 0.1 add setgray
	} repeat
	pop
} def

/center-show {
dup stringwidth
exch neg 2 div exch neg 2 div % negate and divide by two
rmoveto show } def

%statusdict begin
%/pagecount pagecount def
%end
